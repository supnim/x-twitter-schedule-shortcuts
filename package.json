{
	"name": "twitter-schedule-shortcuts",
	"version": "0.1.0",
	"description": "Adds quick 'hours from now' buttons to the X.com scheduling modal",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"check": "biome check .",
		"check:fix": "biome check --write .",
		"fix": "biome check --write --unsafe .",
		"format": "biome format --write .",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write .",
		"typecheck": "tsc --noEmit",
		"reset": "rm -rf node_modules/ && rm -rf dist/ && rm -rf bun.lock && bun install"
	},
	"dependencies": {
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-slot": "^1.1.0",
		"@tailwindcss/vite": "^4.1.17",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"lucide-react": "^0.460.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"tailwind-merge": "^2.5.4"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@crxjs/vite-plugin": "^2.0.0-beta.33",
		"@types/chrome": "^0.0.279",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"@vitejs/plugin-react": "^4.3.3",
		"autoprefixer": "^10.4.20",
		"postcss": "^8.4.49",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.6.3",
		"vite": "^5.4.11"
	}
}
